#include <stdio.h>
#include <dirent.h>

int main() {
    DIR *dir;
    struct dirent *d;

    dir = opendir(".");
    if (dir == NULL) {
        printf("Unable to open directory\n");
        return 1;
    }

    printf("Contents of current directory:\n");

    while ((d = readdir(dir)) != NULL) {
        printf("%s\n", d->d_name);
    }

    closedir(dir);
    return 0;
}
