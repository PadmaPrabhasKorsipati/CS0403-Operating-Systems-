#include <stdio.h>

int main() {
    int start, length;

    printf("Enter starting block: ");
    scanf("%d", &start);
    printf("Enter number of blocks: ");
    scanf("%d", &length);

    printf("Sequentially allocated blocks:\n");
    for (int i = 0; i < length; i++)
        printf("Block %d\n", start + i);

    return 0;
}
